<%= turbo_frame_tag "project" do %>
<% if user_signed_in? %>
    <h1>
      New Project
    </h1>
    <div class="container-fluid modal-body bg-light d-flex align-items-center">
      <%= render "form", project: @project %>
    </div>
	</div>
<% else %>
<%= render 'page_not_found' %>
<% end %>
<% end %>
